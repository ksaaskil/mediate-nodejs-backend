{
  "name": "heroku-example",
  "version": "1.0.0",
  "description": "Small example of a Heroku pipeline",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./build/index.js",
    "clean": "rm -rf build && mkdir build",
    "build": "babel -d ./build ./src -s",
    "dev": "nodemon --exec babel-node src/index.js",
    "test": "jest"
  },
