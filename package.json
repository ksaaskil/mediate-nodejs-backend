  "license": "ISC",
  "dependencies": {
    "express": "^4.17.0",
    "unmock": "^0.0.55",
    "unmock-node": "^0.0.55"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1"
  }
}
